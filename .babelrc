{
  "presets": [
    ["@babel/env", {
      "targets": {
        "browsers": [
          "last 5 versions",
          "ie >= 8"
        ]
      }
    }], "@babel/preset-react"],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    [
      "babel-plugin-styled-components",
      {
        "ssr": false,
        "displayName": true
      }
    ]
  ]
}